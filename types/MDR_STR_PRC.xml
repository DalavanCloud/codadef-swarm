<?xml version="1.0"?>
<cd:Record format="binary" name="MDR_STR_PRC" last-modified="2013-03-07" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Field name="MDR_ID">
    <cd:Integer>
      <cd:Description>Data Set Record identifier</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
      <ct:NamedTest id="FixedValueMDRID0303"/>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="SyncStatus">
    <cd:Integer>
      <cd:Description>Time synchronization status, source and quality</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="t">
    <cd:Time format="binary" timeformat="float(./day) * 86400 + float(./sec) + float(./microsec) * 1e-6">
      <cd:Record>
        <cd:Description>Timestamp of STR Level 1b data</cd:Description>
        <cd:Field name="day">
          <cd:Integer>
            <cd:Unit>days since 2000-01-01</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>int32</cd:NativeType>
            <ct:NamedTest id="RangeDay"/>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="sec">
          <cd:Integer>
            <cd:Unit>s</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>uint32</cd:NativeType>
            <ct:NamedTest id="RangeSecond"/>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="microsec">
          <cd:Integer>
            <cd:Unit>1E-6 s</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>uint32</cd:NativeType>
            <ct:NamedTest id="RangeMicroSecond"/>
          </cd:Integer>
        </cd:Field>
      </cd:Record>
    </cd:Time>
  </cd:Field>
  <cd:Field name="q_ITRF_ICRF">
    <cd:Array>
      <cd:Description>Rotation from ICRF to ITRF frame</cd:Description>
      <cd:Dimension>4</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>int32</cd:NativeType>
        <cd:Conversion numerator="1" denominator="1E9"/>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="q_CRF_ICRF">
    <cd:Array>
      <cd:Description>Rotation from ICRF to CRF frame</cd:Description>
      <cd:Dimension>4</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>int32</cd:NativeType>
        <cd:Conversion numerator="1" denominator="1E9"/>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="q_err">
    <cd:Integer>
      <cd:Description>Uncertainty estimate of q_CRF_ICRF</cd:Description>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
      <cd:Conversion numerator="1" denominator="1E4"/>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="Flags_q">
    <cd:Integer>
      <cd:Description>Flags characterizing the attitude information</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="Fill_1">
    <cd:Raw>
      <cd:Description>Filler</cd:Description>
      <cd:BitSize>24</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
</cd:Record>
