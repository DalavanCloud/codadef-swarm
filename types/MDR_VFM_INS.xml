<?xml version="1.0"?>
<cd:Record format="binary" name="MDR_VFM_INS" last-modified="2013-07-22" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Field name="MDR_ID">
    <cd:Integer>
      <cd:Description>Data Set Record identifier</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
      <ct:NamedTest id="FixedValueMDRID0202"/>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="SyncStatus">
    <cd:Integer>
      <cd:Description>Time synchronization status, source and quality</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="t_VFM">
    <cd:Time format="binary" timeformat="float(./day) * 86400 + float(./sec) + float(./microsec) * 1e-6">
      <cd:Record>
        <cd:Description>Time of VFM Level 1b Instrument level data.</cd:Description>
        <cd:Field name="day">
          <cd:Integer>
            <cd:Unit>days since 2000-01-01</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>int32</cd:NativeType>
            <ct:NamedTest id="RangeDay"/>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="sec">
          <cd:Integer>
            <cd:Unit>s</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>uint32</cd:NativeType>
            <ct:NamedTest id="RangeSecond"/>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="microsec">
          <cd:Integer>
            <cd:Unit>1E-6 s</cd:Unit>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>uint32</cd:NativeType>
            <ct:NamedTest id="RangeMicroSecond"/>
          </cd:Integer>
        </cd:Field>
      </cd:Record>
    </cd:Time>
  </cd:Field>
  <cd:Field name="VFM">
     <cd:Array>
      <cd:Dimension>50</cd:Dimension>
      <cd:Dimension>3</cd:Dimension>
      <cd:Description>VFM measurements converted using CCDB.VFM.Cal parameters, not corrected for stray fields</cd:Description>
      <cd:Integer>
        <cd:Unit>1E-4 nT</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>int32</cd:NativeType>
        <cd:Conversion numerator="1" denominator="1E4">
          <cd:Unit>nT</cd:Unit>
        </cd:Conversion>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
