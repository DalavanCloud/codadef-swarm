<?xml version="1.0"?>
<cd:Record format="xml" last-modified="2014-08-25" name="SPH_ASMVFM_1B" namexml="SPH" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Description>Specific Product Header for ASMXAUX_1B and VFMXAUX_1B products</cd:Description>
  <cd:Field name="SPH_Descriptor">
    <cd:Text namexml="SPH_Descriptor"> </cd:Text>
  </cd:Field>
  <cd:Field name="Orbit_Information">
    <cd:Record namexml="Orbit_Information">
      <cd:Field name="Sensing_Start">
        <cd:Time timeformat="time(str(.),&quot;'UTC='yyyy-MM-dd'T'HH:mm:ss.SSSSSS&quot;)">
          <cd:Description>UTC start time of sensing</cd:Description>
          <cd:Text namexml="Sensing_Start"/>
          <cd:Mapping string="UTC=0000-00-00T00:00:00.000000" value="-inf"/>
          <cd:Mapping string="UTC=9999-99-99T99:99:99.999999" value="+inf"/>
          <cd:Mapping string="" value="nan"/>
        </cd:Time>
      </cd:Field>
      <cd:Field name="Sensing_Stop">
        <cd:Time timeformat="time(str(.),&quot;'UTC='yyyy-MM-dd'T'HH:mm:ss.SSSSSS&quot;)">
          <cd:Description>UTC stop time of sensing</cd:Description>
          <cd:Text namexml="Sensing_Stop"/>
          <cd:Mapping string="UTC=0000-00-00T00:00:00.000000" value="-inf"/>
          <cd:Mapping string="UTC=9999-99-99T99:99:99.999999" value="+inf"/>
          <cd:Mapping string="" value="nan"/>
        </cd:Time>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="Maneuver_Information">
    <cd:Record namexml="Maneuver_Information">
      <cd:Attribute name="count"/>
      <cd:Field name="Maneuver_Id">
        <cd:Array>
          <cd:Dimension/>
          <cd:Type namexml="Maneuver_Id"> 
            <cd:Integer format="ascii">
              <cd:Description>Maneuver identification code</cd:Description>
              <cd:NativeType>int16</cd:NativeType>
              <cd:ByteSize>3</cd:ByteSize>
            </cd:Integer>
          </cd:Type>
        </cd:Array>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="Magnetic_Stray_Fields">
    <cd:Record namexml="Magnetic_Stray_Fields">
      <cd:Field name="VFM_q">
        <cd:Record namexml="VFM_q">
          <cd:Description>Transformation from S/C to VFM sensor frame, CCDB.Structure.VFM_q</cd:Description>
          <cd:Field name="Q1">
            <cd:Type namexml="Q1">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q2">
            <cd:Type namexml="Q2">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q3">
            <cd:Type namexml="Q3">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q4">
            <cd:Type namexml="Q4">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
        </cd:Record>
      </cd:Field>
      <cd:Field name="ASM_q_VFM">
        <cd:Record namexml="ASM_q_VFM">
          <cd:Description>Transformation from VFM sensor to ASM sensor frame, CCDB.Structure.ASM_q_VFM</cd:Description>
          <cd:Field name="Q1">
            <cd:Type namexml="Q1">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q2">
            <cd:Type namexml="Q2">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q3">
            <cd:Type namexml="Q3">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
          <cd:Field name="Q4">
            <cd:Type namexml="Q4">
              <cd:Float format="ascii">
                <cd:NativeType>double</cd:NativeType>
              </cd:Float>
            </cd:Type>
          </cd:Field>
        </cd:Record>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="Product_Confidence_Data">
    <cd:Record namexml="Product_Confidence_Data">
      <cd:Field name="Quality_Indicator">
        <cd:Type namexml="Quality_Indicator">
          <cd:Integer format="ascii">
            <cd:Description>General product quality indicator</cd:Description>
            <cd:NativeType>int16</cd:NativeType>
            <cd:ByteSize>3</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="HK_ISP_Missing">
        <cd:Type namexml="HK_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous platform HK ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="GPSR_ISP_Missing">
        <cd:Type namexml="GPSR_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous GPSR ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="STR_ISP_Missing">
        <cd:Type namexml="STR_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous STR ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="VFM_ISP_Missing">
        <cd:Type namexml="VFM_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous VFM ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="MTR_ISP_Missing">
        <cd:Type namexml="MTR_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous Magnetic Torquer HK ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
      <cd:Field name="Bus_ISP_Missing">
        <cd:Type namexml="Bus_ISP_Missing">
          <cd:Integer format="ascii">
            <cd:Description>Number of missing/erroneous Bus Current HK ISPs</cd:Description>
            <cd:NativeType>uint32</cd:NativeType>
            <cd:ByteSize>5</cd:ByteSize>
          </cd:Integer>
        </cd:Type>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="List_of_DSDs">
    <cd:NamedType id="List_of_DSDs"/>
    <ct:NamedTest id="ByteOrderValid" path="DSD[]/Byte_Order" />
    <ct:NamedTest id="FixedValueByteOrder3210ForMDSD" path="DSD[]/Byte_Order" />
  </cd:Field>
</cd:Record>
